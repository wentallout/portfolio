{
  "name": "portfolio",
  "type": "module",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wentallout/portfolio"
  },
  "bugs": {
    "url": "https://github.com/wentallout/portfolio/issues"
  },
  "scripts": {
    "dev": "vite dev --host --open",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "preview": "vite preview",
    "check": "svelte-check",
    "check:watch": "svelte-check --watch",
    "lint:stylelint": "stylelint \"src/**/*.{css,svelte}\"",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "ngrok": "ngrok http 4173",
    "postbuild": "npx svelte-sitemap --domain https://wentallout.io.vn",
    "postinstall": "paraglide-js compile --project ./project.inlang",
    "test:chrome": "npx playwright test --headed --browser=chromium",
    "test:firefox": "npx playwright test --headed --browser=firefox",
    "test:safari": "npx playwright test --headed --browser=webkit"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^2.1.1",
    "@iconify-json/ph": "^1.1.13",
    "@inlang/paraglide-js": "^1.8.0",
